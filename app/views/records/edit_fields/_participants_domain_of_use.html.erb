<%= f.input :participants_domain_of_use, as: :check_boxes,
    collection: GenericLocalAuthorityService.select_all_options('domains_of_use'),
    value_method: lambda {|v| v.last.to_s },
    input_html: { multiple: true } %>

<div id="participants_domain_of_use_other_div" style="display: none; position: relative; top: -20px;">
  <%= f.input :participants_domain_of_use_other,
              input_html: { class: 'form-control' }
  %>
</div>